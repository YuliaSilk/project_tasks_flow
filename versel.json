{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/" 
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.ts"
    }
  ]
}